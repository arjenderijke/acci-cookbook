<?xml version='1.0' encoding='UTF-8'?>
<hudson.tasks.Mailer_-DescriptorImpl plugin="mailer@1.11">
  <defaultSuffix><%= @mailer['default-suffix'] %></defaultSuffix>
  <hudsonUrl>http://<%= @mailer['public-dns'] %>:8080/</hudsonUrl>
  <smtpAuthUsername><%= @mailer['smtp-username'] %></smtpAuthUsername>
  <smtpAuthPassword><%= @mailer['smtp-password'] %></smtpAuthPassword>
  <replyToAddress><%= @mailer['reply-to'] %></replyToAddress>
  <smtpHost><% @mailer['smtp-host'] %></smtpHost>
  <useSsl>false</useSsl>
  <smtpPort><%= @mailer['smtp-port'] %></smtpPort>
  <charset>UTF-8</charset>
</hudson.tasks.Mailer_-DescriptorImpl>
